{
  "name": "mongodb-querystats-webapp",
  "version": "1.0.0",
  "description": "React/Node.js app for viewing MongoDB query statistics",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd client && npm start",
    "client:build": "cd client && npm run build",
    "client:install": "cd client && npm install",
    "install:all": "npm install && npm run client:install",
    "dev:all": "concurrently \"npm run dev\" \"npm run client\"",
    "build": "npm run client:build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:connection": "node test-connection.js",
    "test:runids": "node test-runids.js"
  },
  "keywords": [
    "mongodb",
    "react",
    "nodejs",
    "querystats",
    "database",
    "viewer"
  ],
  "author": "Brady Byrd",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "mongodb": "^6.3.0",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
